{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es6",
      "dom"
     ],
    "jsx": "react",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "outDir": "lib/",
    "module": "CommonJS",
    "declaration": true,
    "sourceMap": true
  }
}
